  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "knex": "^0.95.11",
    "knex-cleaner": "^1.3.1",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
